# Environment variables for MLA-1
# Copy this file to .env and fill in your values

# Hyperliquid API credentials (required)
HYPERLIQUID_API_KEY=your_api_key_here
HYPERLIQUID_API_SECRET=your_api_secret_here
HYPERLIQUID_WALLET_ADDRESS=your_wallet_address_here

# Optional: Override default capital
MLA_INITIAL_CAPITAL=1000

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
MLA_LOG_LEVEL=INFO

# Optional: Data directory
MLA_DATA_DIR=./data

# Optional: Model directory  
MLA_MODEL_DIR=./models
